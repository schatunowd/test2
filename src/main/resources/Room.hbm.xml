<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping">
    <class name="model.Room" table="room">
        <id name = "id_room" type = "int" column = "id_room">
            <generator class="identity"/>
        </id>
        <property name = "floor" column = "floor"/>
        <property name = "square" column = "square"/>
        <property name = "temperature" column = "temperature"/>
        <property name = "humidity" column = "humidity"/>
    </class>
</hibernate-mapping>